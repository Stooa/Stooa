{
  "name": "stooa-app",
  "license": "MPL-2.0",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect --no-experimental-fetch' next dev",
    "dev:test": "NODE_ENV=test npm run dev",
    "build": "next build",
    "build:test": "NODE_ENV=test next build",
    "start": "next start",
    "build-start": "next build && next start",
    "design-tokens": "design-tokens",
    "test": "jest",
    "test-watch": "jest --watch",
    "prettify": "prettier --write src tests",
    "lint:js": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "mock-server": "npx tsx ./src/mocks/http.ts",
    "e2e:mock:test:dev": "concurrently -n Dev,Cypress,MockServer -c blue,yellow,green \"npm:dev:test\" \"npm:test:e2e:dev\" \"npm:mock-server\" ",
    "e2e:mock:test:run": "concurrently -n Dev,Cypress,MockServer -c blue,yellow,green \"npm:dev:test\" \"npm:test:e2e:run\" \"npm:mock-server\" ",
    "test:e2e:dev": "cypress open",
    "test:e2e:run": "cypress run --browser chrome"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "tests/e2e/features/**/*.ts"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@apollo/client": "3.8.10",
