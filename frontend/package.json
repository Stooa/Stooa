    "lib-jitsi-meet": "https://github.com/jitsi/lib-jitsi-meet/releases/download/v1530.0.0+f2af389e/lib-jitsi-meet.tgz",
    "lottie-web": "5.10.2",
    "next": "13.5.6",
    "next-translate": "1.6.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
