{
    "name": "stooa/stooa",
    "description": "Stooa Project",
    "license": "MPL-2.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "api-platform/core": "2.6.8",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.6.3",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.12.2",
        "gedmo/doctrine-extensions": "3.7.0",
        "gesdinet/jwt-refresh-token-bundle": "1.0.1",
        "hashids/hashids": "4.1.0",
        "lexik/jwt-authentication-bundle": "2.15.1",
        "nelmio/cors-bundle": "2.2.0",
        "ramsey/uuid": "4.3.1",
        "ramsey/uuid-doctrine": "1.8.1",
        "rector/rector": "0.12.23",
        "sonata-project/admin-bundle": "4.12.0",
        "sonata-project/doctrine-extensions": "1.17.0",
        "sonata-project/doctrine-orm-admin-bundle": "4.3.0",
        "sonata-project/exporter": "2.12.0",
        "sonata-project/form-extensions": "1.16.0",
        "sonata-project/twig-extensions": "1.10.0",
        "sonata-project/user-bundle": "5.1.0",
        "symfony/amazon-mailer": "5.4.8",
        "symfony/asset": "5.4.7",
        "symfony/config": "4.4.41",
        "symfony/console": "4.4.41",
        "symfony/dotenv": "5.4.5",
        "symfony/expression-language": "5.4.8",
        "symfony/finder": "5.4.8",
        "symfony/flex": "1.18.7",
        "symfony/framework-bundle": "4.4.41",
        "symfony/http-client": "5.4.8",
        "symfony/intl": "5.4.8",
        "symfony/mailer": "5.4.8",
        "symfony/messenger": "5.4.8",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/security-bundle": "4.4.41",
        "symfony/string": "5.4.8",
        "symfony/translation": "4.4.41",
        "symfony/twig-bundle": "4.4.41",
        "symfony/validator": "4.4.41",
        "symfony/workflow": "5.4.8",
        "symfony/yaml": "5.3.14",
        "symfonycasts/reset-password-bundle": "1.13.0",
        "twig/twig": "3.3.10",
        "webmozart/assert": "1.10.0",
        "webonyx/graphql-php": "14.11.6"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.5.3",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "ergebnis/composer-normalize": "2.26.0",
        "friendsofphp/php-cs-fixer": "3.4.0",
        "phpstan/phpstan": "1.7.15",
        "phpstan/phpstan-doctrine": "1.3.9",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpstan/phpstan-symfony": "1.2.5",
        "phpstan/phpstan-webmozart-assert": "1.2.0",
        "phpunit/phpunit": "9.5.20",
        "psalm/plugin-phpunit": "0.16.1",
        "psalm/plugin-symfony": "3.1.5",
        "runroom-packages/testing": "0.15.7",
        "symfony/browser-kit": "5.4.3",
        "symfony/debug-bundle": "4.4.37",
        "symfony/maker-bundle": "1.39.0",
        "symfony/phpunit-bridge": "5.4.8",
        "symfony/stopwatch": "5.4.5",
        "symfony/var-dumper": "5.4.8",
        "symfony/web-profiler-bundle": "4.4.41",
        "vimeo/psalm": "4.23.0",
        "weirdan/doctrine-psalm-plugin": "2.3.0",
        "zenstruck/foundry": "1.19.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4 || ^5.3"
        }
    },
    "scripts": {
        "lint-container": "@php bin/console lint:container",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
