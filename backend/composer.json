{
    "name": "stooa/stooa",
    "description": "Stooa Project",
    "license": "MPL-2.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-apcu": "*",
        "api-platform/core": "3.1.4",
        "doctrine/annotations": "2.0.1",
        "doctrine/doctrine-bundle": "2.8.3",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.14.1",
        "gedmo/doctrine-extensions": "3.11.1",
        "gesdinet/jwt-refresh-token-bundle": "1.1.1",
        "hashids/hashids": "4.1.0",
        "lexik/jwt-authentication-bundle": "2.18.1",
        "nelmio/cors-bundle": "2.3.1",
        "paragonie/halite": "5.1.0",
        "ramsey/uuid": "4.7.3",
        "ramsey/uuid-doctrine": "2.0.0",
        "rector/rector": "0.15.21",
        "sonata-project/admin-bundle": "4.22.4",
        "sonata-project/doctrine-extensions": "2.1.0",
        "sonata-project/doctrine-orm-admin-bundle": "4.9.1",
        "sonata-project/exporter": "3.1.1",
        "sonata-project/form-extensions": "1.18.1",
        "sonata-project/twig-extensions": "2.0.0",
        "sonata-project/user-bundle": "5.5.0",
        "symfony/amazon-mailer": "6.2.7",
        "symfony/asset": "6.2.7",
        "symfony/config": "6.2.7",
        "symfony/console": "6.2.7",
        "symfony/doctrine-messenger": "6.2.7",
        "symfony/dotenv": "6.2.7",
        "symfony/expression-language": "6.2.7",
        "symfony/finder": "6.2.7",
        "symfony/flex": "2.2.5",
        "symfony/framework-bundle": "6.2.7",
        "symfony/http-client": "6.2.7",
        "symfony/intl": "6.2.7",
        "symfony/mailer": "6.2.7",
        "symfony/messenger": "6.2.7",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/runtime": "6.2.7",
        "symfony/security-bundle": "6.2.7",
        "symfony/string": "6.2.7",
        "symfony/translation": "6.2.7",
        "symfony/twig-bundle": "6.2.7",
        "symfony/validator": "6.2.7",
        "symfony/workflow": "6.2.7",
        "symfony/yaml": "6.2.7",
        "symfonycasts/reset-password-bundle": "1.17.0",
        "twig/twig": "3.5.1",
        "webmozart/assert": "1.11.0",
        "webonyx/graphql-php": "14.11.9",
        "zenstruck/foundry": "1.29.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.6.3",
        "ergebnis/composer-normalize": "2.30.2",
        "friendsofphp/php-cs-fixer": "3.14.5",
        "phpstan/phpstan": "1.10.6",
        "phpstan/phpstan-doctrine": "1.3.35",
        "phpstan/phpstan-phpunit": "1.3.10",
        "phpstan/phpstan-symfony": "1.2.23",
        "phpstan/phpstan-webmozart-assert": "1.2.4",
        "phpunit/phpunit": "10.0.15",
        "psalm/plugin-phpunit": "0.18.4",
        "psalm/plugin-symfony": "5.0.1",
        "symfony/browser-kit": "6.2.7",
        "symfony/debug-bundle": "6.2.7",
        "symfony/maker-bundle": "1.48.0",
        "symfony/phpunit-bridge": "6.2.7",
        "symfony/stopwatch": "6.2.7",
        "symfony/var-dumper": "6.2.7",
        "symfony/web-profiler-bundle": "6.2.7",
        "vimeo/psalm": "5.8.0",
        "weirdan/doctrine-psalm-plugin": "2.8.1"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Core\\": "packages/Core/src/",
            "App\\Fishbowl\\": "packages/Fishbowl/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Core\\Tests\\": "packages/Core/tests/",
            "App\\Fishbowl\\Tests\\": "packages/Fishbowl/tests/",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.1"
        }
    },
    "scripts": {
        "lint-container": "@php bin/console lint:container",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
