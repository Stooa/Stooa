{
    "name": "stooa/stooa",
    "description": "Stooa Project",
    "license": "MPL-2.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "api-platform/core": "2.6.8",
        "doctrine/annotations": "1.13.3",
        "doctrine/doctrine-bundle": "2.7.0",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.13.1",
        "gedmo/doctrine-extensions": "3.9.0",
        "gesdinet/jwt-refresh-token-bundle": "1.1.1",
        "hashids/hashids": "4.1.0",
        "lexik/jwt-authentication-bundle": "2.16.0",
        "nelmio/cors-bundle": "2.2.0",
        "paragonie/halite": "5.1.0",
        "ramsey/uuid": "4.5.1",
        "ramsey/uuid-doctrine": "1.8.1",
        "rector/rector": "0.14.2",
        "sonata-project/admin-bundle": "4.18.0",
        "sonata-project/doctrine-extensions": "1.18.1",
        "sonata-project/doctrine-orm-admin-bundle": "4.7.0",
        "sonata-project/exporter": "2.13.0",
        "sonata-project/form-extensions": "1.18.0",
        "sonata-project/twig-extensions": "1.12.0",
        "sonata-project/user-bundle": "5.3.3",
        "symfony/amazon-mailer": "6.1.0",
        "symfony/asset": "6.1.0",
        "symfony/config": "6.1.3",
        "symfony/console": "6.1.4",
        "symfony/doctrine-messenger": "6.1.4",
        "symfony/dotenv": "6.1.0",
        "symfony/expression-language": "6.1.3",
        "symfony/finder": "6.1.3",
        "symfony/flex": "2.2.3",
        "symfony/framework-bundle": "6.1.4",
        "symfony/http-client": "6.1.4",
        "symfony/intl": "6.1.0",
        "symfony/mailer": "6.1.4",
        "symfony/messenger": "6.1.3",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/runtime": "6.1.3",
        "symfony/security-bundle": "6.1.3",
        "symfony/string": "6.1.4",
        "symfony/translation": "6.1.4",
        "symfony/twig-bundle": "6.1.1",
        "symfony/validator": "6.1.4",
        "symfony/workflow": "6.1.3",
        "symfony/yaml": "6.1.4",
        "symfonycasts/reset-password-bundle": "1.13.0",
        "twig/twig": "3.4.2",
        "webmozart/assert": "1.11.0",
        "webonyx/graphql-php": "14.11.8"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.5.3",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "ergebnis/composer-normalize": "2.28.3",
        "friendsofphp/php-cs-fixer": "3.11.0",
        "phpstan/phpstan": "1.8.5",
        "phpstan/phpstan-doctrine": "1.3.14",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpstan/phpstan-symfony": "1.2.13",
        "phpstan/phpstan-webmozart-assert": "1.2.1",
        "phpunit/phpunit": "9.5.24",
        "psalm/plugin-phpunit": "0.17.0",
        "psalm/plugin-symfony": "3.1.9",
        "runroom-packages/testing": "0.16.0",
        "symfony/browser-kit": "6.1.3",
        "symfony/debug-bundle": "6.1.3",
        "symfony/maker-bundle": "1.45.0",
        "symfony/phpunit-bridge": "6.1.3",
        "symfony/stopwatch": "6.1.0",
        "symfony/var-dumper": "6.1.3",
        "symfony/web-profiler-bundle": "6.1.2",
        "vimeo/psalm": "4.27.0",
        "weirdan/doctrine-psalm-plugin": "2.3.0",
        "zenstruck/foundry": "1.22.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.1"
        }
    },
    "scripts": {
        "lint-container": "@php bin/console lint:container",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
