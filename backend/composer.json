{
    "name": "stooa/stooa",
    "description": "Stooa Project",
    "license": "MPL-2.0",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-apcu": "*",
        "api-platform/core": "3.1.12",
        "doctrine/annotations": "2.0.1",
        "doctrine/doctrine-bundle": "2.10.2",
        "doctrine/doctrine-fixtures-bundle": "3.4.4",
        "doctrine/doctrine-migrations-bundle": "3.2.4",
        "doctrine/orm": "2.16.2",
        "gedmo/doctrine-extensions": "3.12.0",
        "gesdinet/jwt-refresh-token-bundle": "1.1.3",
        "hashids/hashids": "5.0.2",
        "lexik/jwt-authentication-bundle": "2.19.0",
        "metaclass-nl/filter-bundle": "3.0.0",
        "nelmio/cors-bundle": "2.3.1",
        "paragonie/halite": "5.1.0",
        "ramsey/uuid": "4.7.4",
        "ramsey/uuid-doctrine": "2.0.0",
        "rector/rector": "0.18.6",
        "sonata-project/admin-bundle": "4.28.0",
        "sonata-project/doctrine-extensions": "2.3.0",
        "sonata-project/doctrine-orm-admin-bundle": "4.14.0",
        "sonata-project/exporter": "3.3.0",
        "sonata-project/form-extensions": "1.20.0",
        "sonata-project/twig-extensions": "2.3.0",
        "sonata-project/user-bundle": "5.10.0",
        "symfony/amazon-mailer": "6.3.0",
        "symfony/asset": "6.3.0",
        "symfony/config": "6.3.2",
        "symfony/console": "6.3.4",
        "symfony/doctrine-messenger": "6.3.7",
        "symfony/dotenv": "6.3.7",
        "symfony/expression-language": "6.3.0",
        "symfony/finder": "6.3.5",
        "symfony/flex": "2.4.0",
        "symfony/framework-bundle": "6.3.7",
        "symfony/http-client": "6.3.7",
        "symfony/intl": "6.3.7",
        "symfony/mailer": "6.3.5",
        "symfony/messenger": "6.3.7",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/runtime": "6.3.2",
        "symfony/security-bundle": "6.3.7",
        "symfony/string": "6.3.5",
        "symfony/translation": "6.3.7",
        "symfony/twig-bundle": "6.3.0",
        "symfony/validator": "6.3.7",
        "symfony/workflow": "6.3.4",
        "symfony/yaml": "6.3.7",
        "symfonycasts/reset-password-bundle": "1.18.0",
        "twig/twig": "3.7.1",
        "webmozart/assert": "1.11.0",
        "webonyx/graphql-php": "14.11.10",
        "zenstruck/foundry": "1.36.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.6.7",
        "ergebnis/composer-normalize": "2.39.0",
        "friendsofphp/php-cs-fixer": "3.38.0",
        "phpstan/phpstan": "1.10.41",
        "phpstan/phpstan-doctrine": "1.3.46",
        "phpstan/phpstan-phpunit": "1.3.15",
        "phpstan/phpstan-symfony": "1.3.5",
        "phpstan/phpstan-webmozart-assert": "1.2.4",
        "phpunit/phpunit": "10.4.2",
        "psalm/plugin-phpunit": "0.18.4",
        "psalm/plugin-symfony": "5.0.3",
        "symfony/browser-kit": "6.3.2",
        "symfony/debug-bundle": "6.3.2",
        "symfony/maker-bundle": "1.51.1",
        "symfony/phpunit-bridge": "6.3.6",
        "symfony/stopwatch": "6.3.0",
        "symfony/var-dumper": "6.3.6",
        "symfony/web-profiler-bundle": "6.3.6",
        "vimeo/psalm": "5.15.0",
        "weirdan/doctrine-psalm-plugin": "2.9.0"
    },
    "replace": {
        "paragonie/random_compat": "^2.0",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Core\\": "packages/Core/src/",
            "App\\Fishbowl\\": "packages/Fishbowl/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Core\\Tests\\": "packages/Core/tests/",
            "App\\Fishbowl\\Tests\\": "packages/Fishbowl/tests/",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.3"
        }
    },
    "scripts": {
        "lint-container": "@php bin/console lint:container",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
